{
  "name": "ByGo-Backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "lint": "eslint .",
    "format": "prettier --write .",
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "node tools/cleanup cjs && tsc -p buildconfig/tsconfig.cjs.json",
    "build:esm": "node tools/cleanup esm && tsc -p buildconfig/tsconfig.esm.json",
    "build:types": "node tools/cleanup types && tsc -p buildconfig/tsconfig.types.json",
    "dev": "nodemon -r dotenv/config ./dest/cjs/server.js",
    "start": "node -r dotenv/config ./dest/cjs/server.js",
    "test": "jest --coverage",
    "prepare": "husky install",
    "migrate": "ts-node -r dotenv/config ./node_modules/.bin/sequelize db:migrate --env development",
    "migrate:undo": "ts-node -r dotenv/config ./node_modules/.bin/sequelize db:migrate:undo --env development",
    "migrate:undo:all": "ts-node -r dotenv/config ./node_modules/.bin/sequelize db:migrate:undo:all --env development",
    "migrate:status": "ts-node -r dotenv/config ./node_modules/.bin/sequelize db:migrate:status --env development",
    "migrate:create": "ts-node -r dotenv/config ./node_modules/.bin/sequelize migration:generate --env development --name"
  },
  "repository": "https://github.com/nitishm2002/Bygo-Backend.git",
  "author": "Nitish",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.962.0",
    "@aws-sdk/lib-storage": "^3.962.0",
    "@aws-sdk/s3-request-presigner": "^3.962.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "fontkit": "^2.0.4",
    "i": "^0.3.7",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.10",
    "pdf-lib": "^1.17.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "redis": "^5.10.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "socket.io": "^4.8.3",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.11.1",
    "uuid": "^8.3.2",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@eslint/config-array": "^0.23.0",
    "@eslint/object-schema": "^3.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/fontkit": "^2.0.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.4",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "multer": "^2.0.2",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
